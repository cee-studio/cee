      [
        { "name": "code", "type":{ "base":"char", "dec":"*" }, "comment":"@todo fixed size limit"},
        { "name": "guild", "type":{ "base":"guild::dati", "dec":"*"}, "comment":"partial guild object"},
        { "name": "channel", "type":{ "base":"channel::dati", "dec":"*"}, "comment":"partial channel object"},
